#include<iostream>
#include <iostream>
#include <vector>       
#include <string> 
#include <algorithm>
using namespace std;
class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int n = nums.size();
        for (int i = 0; i < n; i++) {
            for (int j = i + 1; j < n; j++) {
                if (nums[i] + nums[j] == target) {
                    return {i, j};
}
}
}
return {}; 
}
};

int main() {
    Solution sol;
    int t = 5;
    vector<int> nums = {1, 2, 0, 1};
    vector<int> result = sol.twoSum(nums, t);

    if (!result.empty()) {
        cout << "Indices: " << result[0] << ", " << result[1] << endl;
    } else {
        cout << "No two numbers add up to the target." << endl;
    }

    return 0;
}
